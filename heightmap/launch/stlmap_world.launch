<launch>

  <param name="/use_sim_time" value="true" />

  <node name="gazebo" pkg="gazebo" type="gazebo" args="$(find heightmap)/worlds/stlmesh.world" respawn="false" output="screen">
  	<env name="GAZEBO_RESOURCE_PATH" value="$(find gazebo)/gazebo/share/gazebo-1.2/media/"/>
  </node>

  <node name="gazebo_gui" pkg="gazebo" type="gui" respawn="false" output="screen"/>

</launch>
